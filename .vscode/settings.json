{
  "editor.codeActionsOnSave": {
    // "source.organizeImports": true,
    "source.fixAll": true,
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/node_modules": true,
    "yarn.lock": true
  },
  "files.eol": "\n",
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative"
}
