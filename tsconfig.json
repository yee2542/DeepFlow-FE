{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "outDir": "./dist/",
    "baseUrl": "./src",
    "paths": {
      "Apps/*": ["apps/*"],
      "Assets/*": ["assets/*"],
      "Components/*": ["components/*"],
      "Hooks/*": ["hooks/*"],
      "Models/*": ["models/*"],
      "Pages/*": ["pages/*"],
      "Routers/*": ["routers/*"],
      "Services/*": ["services/*"],
      "Stores/*": ["stores/*"],
      "Utils/*": ["utils/*"]
    }
  },

  "include": ["src"]
}
